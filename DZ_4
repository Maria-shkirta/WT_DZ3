
-- create
CREATE TABLE EMPLOYEE (
  Id INTEGER PRIMARY KEY,
  name TEXT NOT NULL,
  age TEXT NOT NULL,
  address TEXT NOT NULL
);

-- insert
INSERT INTO EMPLOYEE VALUES (0001, 'Albert', '34','Moscow, Sadovaya 1');
INSERT INTO EMPLOYEE VALUES (0002, 'Anay', '25','S-Petersburg, Nevsky 1');
INSERT INTO EMPLOYEE VALUES (0003, 'Evgenyi', '35', 'Sochi, Imeretinskaya nab 1');

-- fetch 
SELECT * FROM EMPLOYEE WHERE age>18;
